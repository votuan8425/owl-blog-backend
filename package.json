{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Minh Tuan Dev",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "server": "nodemon dist/index.js",
    "start-ts": "ts-node src/index.ts",
    "server-ts": "nodemon --exec ts-node src/index.ts",
    "watch": "tsc -w",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/config/data-source.ts",
    "migration:generate": "yarn run typeorm:cli migration:generate"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.4",
    "@types/micro-cors": "^0.1.3",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.16.3",
    "@types/nodemailer": "^6.4.7",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@typegoose/typegoose": "^11.1.0",
    "@types/uuid": "8.3.1",
    "apollo-server": "^3.12.0",
    "apollo-server-core": "^3.12.0",
    "apollo-server-env": "^4.2.1",
    "apollo-server-express": "^3.12.0",
    "apollo-server-micro": "^3.12.0",
    "argon2": "0.28.2",
    "class-validator": "0.13.1",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "cross-var": "^1.1.0",
    "dataloader": "2.0.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "google-auth-library": "^8.8.0",
    "googleapis": "^118.0.0",
    "graphql": "15.5.1",
    "micro": "^10.0.1",
    "micro-cors": "^0.1.1",
    "mongoose": "^7.1.0",
    "nodemailer": "^6.9.1",
    "nodemailer-mailgun-transport": "^2.1.5",
    "pg": "8.6.0",
    "reflect-metadata": "0.1.13",
    "tsconfig-paths": "^4.2.0",
    "type-graphql": "1.1.1",
    "typeorm": "^0.3.16",
    "uuid": "8.3.2"
  }
